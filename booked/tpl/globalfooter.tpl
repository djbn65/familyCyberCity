   });
   var $icon = $("#hamburger");
   var API = $menu.data( "mmenu" );

   $icon.on( "click", function( event ) {
     event.preventDefault();
     API.open();
   });

   API.bind( "opened", function() {
     setTimeout(function() {
       $icon.addClass( "is-active" );
     }, 100);
   });
   API.bind( "closed", function() {
     setTimeout(function() {
       $icon.removeClass( "is-active" );
     }, 100);
   });
   </script>

